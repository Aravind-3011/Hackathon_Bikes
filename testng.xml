<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
		<listener class-name="utilities.extentReports"/></listeners>
  <test thread-count="5" name="Chrome">
<parameter name="browser" value="1"></parameter>
	
    <groups>
  <run>
  <include name="smoke"/>
  <include name="regression"/>
  <include name="master"/>
  </run>
  </groups>

    <classes>
      <class name="testBase.testCase"/>
    </classes>
  </test> <!-- Test -->
  
  
  <test thread-count="5" name="Edge">
<parameter name="browser" value="2"></parameter>
	
    <groups>
  <run>
  <include name="smoke"/>
  <include name="regression"/>
  <include name="master"/>
  </run>
  </groups>

    <classes>
      <class name="testBase.testCase"/>
    </classes>
  </test>
 
</suite> <!-- Suite -->
